update_birdseye = function({},
	
	numj = NumberStreets;
	numi = NumberStreets;
	
	Try(gb1[framebox(1)] << Remove Graphics Script( 2 ));
	Try(gb1[framebox(1)] << Remove Graphics Script( 1 ));
	
	str = "gb1[framebox(1)] << Add Graphics Script(";
	
	for(i=0, i<numi, i++,
		for(j=0, j<numj, j++,
			str = str || "rect("|| char(i) || "*(blockdim_x+StreetGap),"|| char(j) || "*(blockdim_y+StreetGap)+10,"|| char(i) || "*(blockdim_x+StreetGap)+10,"|| char(j) || "*(blockdim_y+StreetGap));\!n";
		);
	);
	str = str || ");";

	strparsed = parse(str);
	eval(strparsed);

	gb1[axisbox(1)] << Max( numi*(blockdim_x+StreetGap)+10 );
	gb1[axisbox(2)] << Max( numj*(blockdim_y+StreetGap)+10 );

	gb1 << Reshow;
	//wait(0.01);
	
);
